{
  "schemaVersion": 1,
  "id": "skinshuffle",
  "version": "${version}",
  "name": "SkinShuffle",
  "description": "Choose and change your skin in-game!",
  "authors": [
    "IMB11 (mineblock11)",
    "enjarai"
  ],
  "contact": {
    "repo": "https://github.com/IMB11/skinshuffle"
  },
  "license": "MIT",
  "icon": "assets/skinshuffle/icon.png",
  "accessWidener": "${minecraft_version}.accesswidener",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.mineblock11.skinshuffle.client.SkinShuffleClient"
    ],
    "main": [
      "com.mineblock11.skinshuffle.SkinShuffle"
    ],
    "modmenu": [
      "com.mineblock11.skinshuffle.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "SkinShuffle.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "yacl": ">=${target_yacl}",
    "cicada": ">=${target_cicada}",
    "minecraft": "${target_minecraft}"
  }
}
